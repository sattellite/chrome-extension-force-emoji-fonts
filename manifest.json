{
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_title": "Force Emoji Fonts"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["js/cs.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "description": "Add the emoji color fonts to any page.",
  "icons": {
    "128": "img/logo.png",
    "48": "img/logo48.png"
  },
  "manifest_version": 2,
  "name": "Add emoji color fonts",
  "permissions": ["tabs", "<all_urls>"],
  "version": "1.0.1"
}
